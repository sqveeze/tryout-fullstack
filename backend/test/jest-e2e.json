{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@config": "<rootDir>/../src/config",
    "@types": "<rootDir>/../src/types",
    "@validations": "<rootDir>/../src/validations"
  }
}
